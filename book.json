{
  "title": "mybook",
  "language": "zh-hans",
  "author": "leeson",
  "sortedBy": "-",
  "plugins": [
    "-lunr",
    "-search",
    "search-pro",
    "-sharing",
    "github-buttons",
    "editlink",
    "ga",
    "-highlight",
    "prism",
    "bibtex-indexed-cite",
    "anchor-navigation-ex",
    "page-footer-ex",
    "splitter",
    "todo",
    "copy-code-button",
    "expandable-chapters-small",
    "donate",
    "hide-element",
    "chapter-fold",
    "favicon"
  ],
  "pluginsConfig": {
    "hide-element": {
      "elements": [".gitbook-link"]
    },
    "favicon": {
      "shortcut": "/gitbook.ico",
      "bookmark": "/gitbook.ico"
    },
    "search-pro": {
      "cutWordLib": "nodejieba",
      "defineWord": ["Gitbook Use"]
    },
    "anchor-navigation-ex": {
      "showLevel": false
    },
    "github-buttons": {
      "buttons": [
        {
          "user": "77629296",
          "repo": "play-with-lodash",
          "type": "star",
          "size": "small",
          "count": true
        }
      ]
    },
    "editlink": {
      "base": "https://github.com/77629296/play-with-lodash/blob/master",
      "label": "Edit",
      "multilingual": false
    },
    "ga": {
      "token": ""
    },
    "prism": {
      "ignore": ["mermaid", "eval-js"],
      "css": ["prismjs/themes/prism-tomorrow.css"]
    },
    "page-footer-ex": {
      "copyright": "By [leeson](https://github.com/77629296)",
      "markdown": true,
      "update_label": "此页面修订于：",
      "update_format": "YYYY-MM-DD HH:mm:ss"
    },
    "bibtex-indexed-cite": {
      "path": "/"
    },
    "donate": {
      "wechat": "https://77629296.github.io/play-with-lodash/wechat.png",
      "alipay": "https://77629296.github.io/play-with-lodash/alipay.jpeg",
      "title": "",
      "button": "赏",
      "alipayText": "支付宝打赏",
      "wechatText": "微信打赏"
    }
  }
}
